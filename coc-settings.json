{
	"coc.preferences.extensionUpdateCheck":"daily",
	"suggest.noselect":true,
	"suggest.enablePreselect":true,
	"clangd.semanticHighlighting":true,
	"languageserver": {
        "golang": {
            "command": "gopls",
            "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
            "filetypes": ["go"],
            "initializationOptions": {
                "usePlaceholders": true
            }
        },
    }
}
